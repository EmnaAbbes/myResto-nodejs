{
    "version": 2,
    "name": "nodejs-mongodb",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js",
            "maxDuration": 30
        }
    ],
    "functions": {
        "/app.js": {
            "memory": 3008,
            "maxDuration": 30
        }
    }
}