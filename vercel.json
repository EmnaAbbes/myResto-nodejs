{
    "version": 2,
    "name": "nodejs-mongodb",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "functions": {
        "timeout": 30
      }
}